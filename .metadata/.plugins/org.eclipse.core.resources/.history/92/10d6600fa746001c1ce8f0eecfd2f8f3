package test_p6;

import test_p6.Car;

public class test_p6 
{

	public static void main(String[] args)
	{
		Car car1;
		car1=new Car();
        
		car1.num=1280;
		car1.gas=24.84;
		System.out.println("車號是1"+car1.num);
		System.out.println("汽油輛是"+car1.gas);
	}

}
 class Car
 {
	 int num;
	 double gas;
 }
