package test_p36;

public class test_p36 {

	public static void main(String[] args)
	{
		String str ="HI";
		
		char ch1=str.charAt(0);
		char ch2=str.charAt(1);
		
		int len =str.length();
		
		System.out.print(str+"的第一個字元是"+ch1);
		System.out.print(str+"的第一個字元是"+ch2);
		System.out.print(str+"的長度是"+len);
		
	}

}
