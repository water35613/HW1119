package test_p12;

import test_p12.Car;

public class test_p12 
{

	public static void main(String[] args)
	{
		Car car1;
		car1=new Car();
		
		car1.num=1280;
		car1.gas=24.84;
	
		car1.showCar();
		
		
	}

}
 class Car
 {
	 int num;
	 double gas;
	 
	 void show()
	 {
		    System.out.println("car1車號是"+num);
			System.out.println("car1汽油量是"+gas);
		 
	 }
	 void showCar()
	 {
		 System.out.println("開始顯示車子資料");
		 this.show();
	 }
	 
	 
	 
 }
